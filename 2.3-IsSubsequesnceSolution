function isSubsequence(str1, str2) {
  var i = 0;
  var j = 0;
  if (!str1) return true;
  while (j < str2.length) {
    if (str2[j] === str1[i]) 
    {
        console.log('i- ', i, ' j- ', j)
        console.log('str2[j] - ', str2[j])
        console.log('str1[i] - ', str1[i])
        i++;
    }
    if (i === str1.length) {
    console.log('i---- ',i )
console.log('str1.length---- ',str1.length )
return true;}
    j++;
  }
  return false;
}

isSubsequence('sing','sting')